      ],
      "output": [
        "lib/esm/package.json"
      ]
    },
    "build:docs": {
      "command": "api-extractor run --local --config \"./api-extractor.docs.json\"",
      "files": [
        "api-extractor.docs.json",
        "lib/esm/main.d.ts",
        "tsconfig.json"
      ],
      "dependencies": [
        "build"
      ]
    },
    "build:test": {
      "command": "tsc -b test/src/tsconfig.json",
      "files": [
        "test/**/*.ts",
        "test/src/tsconfig.json"
      ],
      "output": [
        "test/build/**"
      ],
      "dependencies": [
        "build",
        "../testserver:build"
      ]
    },
